{% extends 'base.html.twig' %}

{% block title %}
    - Admin Dashboard
{% endblock %}

{% block body %}
    <h1>Admin Dashboard</h1>

    <p>Welcome to the admin dashboard!</p>

    <h2>Manage Reservations</h2>
    {% for reservation in reservations %}
        <div>
            <h3>{{ reservation }}</h3>
        </div>
    {% endfor %}

    <h2>Manage Vehicle</h2>
    {% for vehicle in vehicles %}
        <div>
            <h3>{{ vehicle }}</h3>
        </div>
    {% endfor %}


{% endblock %}